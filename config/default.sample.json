{
    "defaultSchema": "kassa",
    "defaultLocale": "RU",
    "port": 8089,
    "url": "/comparator/",
    "logLevel": "DEBUG",
    "generalAppCode": "ALL",
    "generalAppDefaultErrorCode": "ALL",
    "test_db": {
        "host": "*",
        "port": 5432,
        "database": "*",
        "user": "*",
        "password": "*"
    },
    "prod_db": {
        "host": "*",
        "port": 5432,
        "database": "*",
        "user": "*",
        "password": "*"
    },
    "comparator_settings": {
        "tablesToCompare": [
            "***", "***"
        ],
        "overrideDefaultSettings": [
            {
